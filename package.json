{
  "name": "chrome-app",
  "version": "1.0.0",
  "description": "Sample application which evaluates the use of Electron and React for desktop application development.",
  "main": "index.js",
  "scripts": {
    "dev:build": "webpack --config webpack.config.dev.js --display-error-details",
    "dev:start": "electron ./dev",
    "dev:watch": "webpack --config webpack.config.dev.js --watch",
    "app:build": "webpack --config webpack.config.prod.js -p --progress",
    "app:distribute": "npm run prod:build && npm run prod:package",
    "app:package": "electron-packager ./app chrome-app --out ./build --overwrite",
    "app:start": "electron ./app"
  },
  "author": "dstrekelj <domagojstrekelj@gmail.com>",
  "license": "Unlicense",
  "devDependencies": {
    "@types/electron": "^1.4.24",
    "@types/history": "^2.0.39",
    "@types/mime": "0.0.29",
    "@types/node": "^6.0.45",
    "@types/react": "^0.14.41",
    "@types/react-dom": "^0.14.18",
    "@types/react-router": "^2.0.38",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "copy-webpack-plugin": "^4.0.0",
    "css-loader": "^0.25.0",
    "electron": "^1.4.3",
    "electron-debug": "^1.0.1",
    "electron-packager": "^8.1.0",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "json-loader": "^0.5.4",
    "mime": "^1.3.4",
    "node-sass": "^3.10.1",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2"
  },
  "dependencies": {
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-router": "^2.8.1"
  }
}
